{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "client": "npm run develop",
    "server": "cd server && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "create-react-class": "^15.6.3",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "gatsby": "^2.19.45",
    "gatsby-plugin-express": "^1.1.6",
    "gatsby-plugin-typography": "^2.4.1",
    "gatsby-source-googlemaps-geocoding": "^1.0.0",
    "gatsby-source-graphql": "^2.4.2",
    "gatsby-source-mongodb": "^2.3.6",
    "google-map-react": "^1.1.7",
    "http": "0.0.1-security",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "net": "^1.0.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0",
    "react-bootstrap-date-picker": "^5.1.0",
    "react-cache": "^2.0.0-alpha.1",
    "react-date-picker": "^8.0.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.12.0",
    "react-geocode": "^0.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-parnassus": "^0.16.19"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
